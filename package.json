{
  "name": "carbon-telegram-bot",
  "version": "0.2.1",
  "description": "Telegram Chatbot that generates carbon.now.sh images from source code",
  "main": "dist/index.js",
  "scripts": {
    "start": "pm2-runtime start ecosystem.config.yml",
    "test": "mocha -r tsnode/register -b test/**/*.test.ts --no-warnings",
    "clean": "rm -rf tsconfig.tsbuildinfo && rm -rf dist",
    "build:watch": "tsc -w",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "start:debug": "tsnd src/index.ts | pino-pretty -c -o '[{context}] {msg}' -t SYS:standard -S -i context"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rjmunhoz/carbon-telegram-bot.git"
  },
  "keywords": [
    "telegram",
    "carbon",
    "syntax",
    "hightlighing",
    "development",
    "presentation"
  ],
  "author": "Rog√©rio Munhoz <hello@rjmunhoz.me>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rjmunhoz/carbon-telegram-bot/issues"
  },
  "homepage": "https://github.com/rjmunhoz/carbon-telegram-bot#readme",
  "dependencies": {
    "@types/node": "^18.0.0",
    "canvas": "^2.9.1",
    "dotenv": "^16.0.1",
    "mongodb": "^4.7.0",
    "ngrok": "^4.3.1",
    "pino": "^8.0.0",
    "shiki": "^0.10.1",
    "shiki-renderer-canvas": "^1.0.1",
    "sugar-env": "^1.5.7",
    "telegraf": "3.33.3",
    "telegraf-inline-menu": "4.0.0"
  },
  "devDependencies": {
    "pino-pretty": "^8.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "nodemon": "^2.0.16"
  }
}